{
  "version": 3,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 900,
        "memory": 1024
      }
    },
    {
      "src": "public",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/generate",
      "dest": "app.js",
      "methods": ["POST"]
    },
    {
      "src": "/modify",
      "dest": "app.js",
      "methods": ["POST"]
    },
    {
      "src": "/models",
      "dest": "app.js",
      "methods": ["GET"]
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/script.js",
      "dest": "/public/script.js"
    },
    {
      "src": "/styles.css",
      "dest": "/public/styles.css"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
